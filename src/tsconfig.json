{
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "target": "ES6",
    "importHelpers": true,
    "module": "ES6",
    "moduleResolution": "Bundler",
    "typeRoots": ["node_modules/@types/web"],
    "lib": ["es5", "es6", "es2017", "dom"],
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "/jsxCore",
    "paths": { "/jsx_core/*": ["./jsxCore/*"], "/*": ["./*"] }
  }
}
