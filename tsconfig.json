{
  "compilerOptions": {
    "rootDir": "src",
    "baseUrl": ".",
    "outDir": "./dist/",
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "Bundler",
    "typeRoots": ["node_modules/@types/web"],
    "lib": ["es5", "es6", "es2017", "dom"],
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@/jsxCore",
    "paths": { "@/*": ["./src/*"] }
  }
}
